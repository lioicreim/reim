	Corrupted True
	Class == "Waystones"
	WaystoneTier >= 1
	WaystoneTier <= 13
	HasExplicitMod >=7 "a" "o" "e" "u" "i" "y"
	SetFontSize 45
	SetTextColor 255 255 255 255
	SetBorderColor 255 255 255 255
	SetBackgroundColor 102 32 32 255
